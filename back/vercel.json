{
    "version": 2,
    "builds": [ 
        { "src": "src/shared/infra/http/server.ts", "use": "@vercel/node" }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "src/shared/infra/http/server.ts" }
    ],
    "rewrites": [
        { "source": "/api/(.*)", "destination": "/src/shared/infra/http/server.ts" }
    ]
}
