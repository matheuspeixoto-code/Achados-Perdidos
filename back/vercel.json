{
    "version": 2,
    "buildCommand": "npm run build",
    "builds": [
        {
            "src": "dist/shared/infra/http/server.js",
            "use": "@vercel/node",
            "config": { "includeFiles": "dist/**" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/shared/infra/http/server.js"
        }
    ]
}
